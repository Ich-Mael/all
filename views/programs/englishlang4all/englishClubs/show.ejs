                              <!-- <legend>Rating</legend> -->
                              <input type="radio" id="no-rate<%= club.media[i]._id %>" class="input-no-rate"
                                name="review[rating]" value="0" checked aria-label="No rating." />
                              <input type="radio" id="first-of-rate1<%= club.media[i]._id %>" name="review[rating]"
