  <!-- Clubs Activities -->
  <h1 class="common-heading">
    <span class="underline"></span>English Learning Activities<span class="underline"></span>
  </h1>
  <div class="main-content-wrapper">
    <div class="container p-2 current-activity">
      <h1 class="current-activity-heading"><span>Upcoming Events of the week</span></h1>
      <div class="container my-5">
        <div class="row">
          <div class="col-md">
            <div class="px-2 my-3 activity-card">
              <h3 class="card-title weekly-activity py-2">English Club Meeting</h3>
              <div class="d-flex meet weekly-content justify-content-center ps-0 flex-column align-items-start">
                <div class="d-flex w-100 meeting-info flex-wrap justify-content-around align-items-center">
                  <h4 class="activity-date">
                    <%= days[club.weeklyMeeting[0].meetingDate.getDay()] %>,
                      <%=club.weeklyMeeting[0].meetingDate.getDate()%>
                        <%= months[club.weeklyMeeting[0].meetingDate.getMonth()] %>
                          <%=club.weeklyMeeting[0].meetingDate.getFullYear()%>
                  </h4>
                  <h4 class="activity-place"> <span class="fw-bold text-primary"><i
                        class="fas me-2 fa-map-marker-alt"></i>Venue : </span>
                    <%=club.weeklyMeeting[0].venue %>
                  </h4>
                  <h4 class="activity-time"> <span class="fw-bold text-primary"><i class="fas me-2 fa-clock"></i>Time:
                    </span>
                    <%=club.weeklyMeeting[0].meetingTime %>
                  </h4>
                </div>
              </div>
              <!-- Button trigger modal -->
              <button type="button" class="btn mx-4 my-2
                                                      btn-outline-success" data-bs-toggle="modal"
                data-bs-target="#wec">
                Learn more
              </button>

              <!-- Modal -->
              <div class="modal fade" id="wec" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg
                                                        modal-fullscreen-md-down">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h6 class="modal-title" id="exampleModalLabel">
                        Agenda
                      </h6>
                      <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                      <p class="fs-4">
                        <%- club.weeklyMeeting[0].about %>
                      </p>
                    </div>
                    <div class="modal-footer">
                      <button type="button" class="btn
                                                              btn-danger" data-bs-dismiss="modal">Close</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md">
            <div class="px-2 my-3 activity-card">

              <h3 class="card-title weekly-activity py-2">Online Debates</h3>
              <div class="d-flex online-deb flex-column align-items-start">
                <div class="weekly-content ps-2 fs-5">
                  <p class="px-2 mt-2"> <span class="me-2 text-white">Topic 1 </span> <span class="fs-6">
                      <%= days[club.onlineDebates[0].debateDateOne.getDay()]%>,
                        <%=club.onlineDebates[0].debateDateOne.getDate()%>
                          <%= months[club.onlineDebates[0].debateDateOne.getMonth()]%>
                            <%=club.onlineDebates[0].debateDateOne.getFullYear()%>
                    </span> At
                    <span class="fs-6">
                      <%=club.onlineDebates[0].debateTimeOne%>
                    </span>
                  </p>
                  <p class=" debate-topic px-2">
                    <%=club.onlineDebates[0].topicOne %>
                  </p>


                  <hr>
                  <p class="px-2"><span class="text-white me-2">Topic 2 </span> <span class="fs-6">
                      <%= days[club.onlineDebates[0].debateDateTwo.getDay()]%>,
                        <%=club.onlineDebates[0].debateDateTwo.getDate()%>
                          <%= months[club.onlineDebates[0].debateDateTwo.getMonth()]%>
                            <%=club.onlineDebates[0].debateDateTwo.getFullYear()%>
                    </span> At <span class="fs-6">
                      <%=club.onlineDebates[0].debateTimeTwo%>
                    </span>
                  </p>
                  <p class="debate-topic px-2">
                    <%=club.onlineDebates[0].topicTwo%>
                  </p>



                </div>
              </div>

              <!-- Button trigger modal -->
              <button type="button" class="btn mx-4 my-2 btn-outline-success" data-bs-toggle="modal"
                data-bs-target="#oed">
                Learn more
              </button>

              <!-- Modal -->
              <div class="modal fade" id="oed" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg
                                                        modal-fullscreen-md-down">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title" id="exampleModalLabel">
                        Online English Debates
                      </h5>
                      <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                      <p>
                        <%- club.onlineDebates[0].aboutTopic %>
                      </p>
                    </div>
                    <div class="modal-footer">
                      <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div>
        </div>
      </div>
    </div>

    <div class="container  col-lg-10">
      <div class="activities-wrapper py-3">
        <!-- Daily vocabulary -->
        <section class="">
          <h1 class="daily-vocabulary-title">Daily Vocabulary</h1>
          <div class="row mb-5">
            <div class="col-md-6">
              <div class="daily-content">
                <div class="card m-2">
                  <div class="card-header text-center word-day">
                    <h4 class="fw-bold ">Word of the day </h4>
                  </div>
                  <div class="card-body">
                    
                      <p class="card-title word-day-title">
                        <%= club.dailyVocab[0].wordOfDay.word %>
                      </p>
                    
                    <p class="card-text word-day-meaning fs-5"><span class="fw-bold">Meaning : </span>
                      <%= club.dailyVocab[0].wordOfDay.wordMeaning %>
                    </p>
                    <!-- Button trigger modal -->
                    <button type="button" class="btn btn-outline-success" data-bs-toggle="modal"
                      data-bs-target="#wod-1">
                      Understand better with examples
                    </button>

                    <!-- Modal -->
                    <div class="modal fade" id="wod-1" tabindex="-1" aria-labelledby="exampleModalLabel"
                      aria-hidden="true">
                      <div class="modal-dialog modal-lg
                                  modal-fullscreen-md-down">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title " id="exampleModalLabel">
                              Understand better with examples
                            </h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                          </div>
                          <div class="modal-body">
                            <p class="fst-italic">Click on the links to learn more</p>
                            <p class="fs-3">
                              <%- club.dailyVocab[0].wordOfDay.learnMoreWord %>
                            </p>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn
                                        btn-danger" data-bs-dismiss="modal">Close</button>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-6">
              <div class="daily-content">
                <div class="card m-2">
                  <div class="card-header text-center idiom-day">
                    <h4 class="fw-bold">Idiom of the day</h4>
                  </div>
                  <div class="card-body">
                      <p class="card-title idioms-title">
                        <%= club.dailyVocab[0].IdiomDay.idiom %>
                      </p>
                    <p class="card-text word-day-meaning fs-5"><span class="fw-bold">Meaning : </span>
                      <%= club.dailyVocab[0].IdiomDay.idiomMeaning %>
                    </p>
                    <!-- Button trigger modal -->
                    <button type="button" class="btn btn-outline-primary" data-bs-toggle="modal"
                      data-bs-target="#wod-2">
                      Understand better with examples
                    </button>

                    <!-- Modal -->
                    <div class="modal fade" id="wod-2" tabindex="-1" aria-labelledby="exampleModalLabel"
                      aria-hidden="true">
                      <div class="modal-dialog modal-lg
                                  modal-fullscreen-md-down">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title " id="exampleModalLabel">
                              Understand better with examples
                            </h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                          </div>
                          <div class="modal-body">
                            <p class="fs-3">
                              <%- club.dailyVocab[0].IdiomDay.learnMoreIdiom %>
                            </p>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn
                                        btn-danger" data-bs-dismiss="modal">Close</button>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>

          </div>
        </section>

        <section id="news-wrapper" class="my-5">
          <h1 class="activity-heading">News</h1>
          <div class="d-flex justifify-content-center">
            <div class="container">
              <div class="row">
                <% for(let i=0; i < 3 ; i++){ %>
                  <div class="col-md-6 col-lg-4">
                    <div class="card news-card">
                      <img src="<%= club.articles[i].articleImage %>" class="card-img-top img-thumbnail" alt="">
                      <div class="card-body">
                        <h5 class="card-title news-title">
                          <%=club.articles[i].title %>
                        </h5>
                        <p class="card-text news-summary">
                          <%= club.articles[i].articleSummary %>
                        </p>
                        <!-- Button trigger modal -->
                        <button type="button" class="btn btn-small
                                      btn-primary" data-bs-toggle="modal"
                          data-bs-target="#N<%= club.articles[i]._id %>">
                          Read more
                        </button>

                        <!-- Modal -->
                        <div class="modal fade" id="N<%= club.articles[i]._id %>" tabindex="-1"
                          aria-labelledby="exampleModalLabel" aria-hidden="true">
                          <div class="modal-dialog modal-lg
                                        modal-fullscreen-md-down">
                            <div class="modal-content">
                              <div class="modal-header">
                                <h5 class="modal-title" id="exampleModalLabel">
                                  <%= club.articles[i].title %>
                                </h5>
                                <button type="button" class="btn-close" data-bs-dismiss="modal"
                                  aria-label="Close"></button>
                              </div>
                              <div class="modal-body">
                                <div>
                                  <%- club.articles[i].articleContent%>
                                </div>
                              </div>
                              <div class="modal-footer">
                                <button type="button" class="btn
                                              btn-danger" data-bs-dismiss="modal">Close</button>
                              </div>
                            </div>
                          </div>
                        </div>
                        <hr>
                        <p class="card-text"><small class="text-muted">
                            <%= days[club.articles[i].postedAT.getDay()] %>,
                              <%=club.articles[i].postedAT.getDate()%>
                                <%= months[club.articles[i].postedAT.getMonth()] %>
                                  <%=club.articles[i].postedAT.getFullYear()%>
                          </small><em class="mx-3"> <br>
                            By
                            <%= club.articles[i].articleAuthor %>
                          </em></p>
                      </div>
                    </div>
                  </div>
                  <% } %>
              </div>
            </div>
        </section>

        <!-- Lessons section -->
        <section id="lesson-wrapper" class="my-5">
          <h1 class="activity-heading">Weekly Lessons</h1>
          <div class="container">
            <div class="lesson-links">
              <div class="row">
                <% for(i=0; i < 2 ; i++){ %>
                  <div class="col-md-6">
                    <div class="lesson-wrapper my-3">
                      <h4 class="week-lesson-title">
                        <%= club.weeklyVideoLesson[i].lessonTitle %>
                      </h4>
                      <p class="">
                        <%- club.weeklyVideoLesson[i].lessonSummary%>
                      </p>
                      <button type="button" class="btn ms-2 video-btn btn-outline-primary" data-bs-toggle="modal"
                        data-bs-target="#lesson-1-vid-<%=i%>">
                        Click to watch the lesson
                      </button>
                    </div>
                  </div>
                  <%}%>
              </div>
              <% for(i=0; i < 2 ; i++){ %>
                <!-- Modal  for video -->
                <div class="modal fade" id="lesson-1-vid-<%=i%>" tabindex="-1" aria-labelledby="exampleModalLabel"
                  aria-hidden="true">
                  <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                      <div class="modal-header">
                        <h5 class="modal-title text-primary" id="exampleModalLabel">
                          <%= club.weeklyVideoLesson[i].lessonTitle %>
                        </h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                      </div>
                      <div class="modal-body">
                        <div id="tv-container">
                          <div id="monitor">
                            <div class="afrohk-TV">
                              <h3 class="text-center afrohk-text mb-2 py-0"><span class="afrohk">AfroHK</span><span
                                  class="tv">Radar</span>
                              </h3>
                            </div>
                            <div id="monitorscreen">
                              <iframe id="vimeo-content" loading="lazy" src="<%= club.weeklyVideoLesson[i].videoLink %>"
                                title="YouTube video player" frameborder="0" height="100%" width="100%"
                                allow="accelerometer; accelerometer; autoplay; clipboard-write; fullscreen; encrypted-media; gyroscope; picture-in-picture"
                                allow="fullscreen" title="High-intensity Classes"></iframe>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="modal-footer">
                        <button type="button" class="btn btn-danger" data-bs-dismiss="modal">
                          Close
                        </button>
                      </div>
                    </div>
                  </div>
                </div>
                <%}%>

            </div>
          </div>
        </section>

        <!-- Weekly activities section -->
