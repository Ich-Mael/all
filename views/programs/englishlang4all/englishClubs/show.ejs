                              <input type="radio" id="first-rate2<%= club.media[i]._id %>" name="review[rating]"
                                value="2" />
                              <label for="first-rate2<%= club.media[i]._id %>" title="Not good">2
                                stars</label>
                              <input type="radio" id="first-rate3<%= club.media[i]._id %>" name="review[rating]"
                                value="3" />
                              <label for="first-rate3<%= club.media[i]._id %>" title="Average">3
                                stars</label>
                              <input type="radio" id="first-rate4<%= club.media[i]._id %>" name="review[rating]"
                                value="4" />
                              <label for="first-rate4<%= club.media[i]._id %>" title="Very good">4
                                stars</label>
                              <input type="radio" id="first-rate5<%= club.media[i]._id %>" name="review[rating]"
                                value="5" />
                              <label for="first-rate5<%= club.media[i]._id %>" title="Amazing">5
                                stars</label>
                            </fieldset>
                            <div class="form-floating">
                              <textarea class="form-control" name="review[text]" placeholder="Leave a comment here"
                                id="floatingTextarea" style="height: 70px" required></textarea>
                              <label for="floatingTextarea">Leave a comment</label>
                            </div>
                            <button class="btn btn-outline-primary my-3">Post Comment</button>
                          </form>
                          <!-- all comments -->
                          <hr>

                          <h5 class="text-center py-3 mb-3" style="color:white; background: #8E2DE2;  /* fallback for old browsers */
