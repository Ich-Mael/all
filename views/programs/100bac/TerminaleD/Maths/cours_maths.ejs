<h1 class="maths-title text-center">Cours de Mathématiques Tle D</h1>
<div class="maths-wrapper d-flex flex-column justify-content-center align-items-center container">
  <div id="course-menu-wrapper" class="col-2 d-flex flex-column justify-content-center align-items-center col-md-4">
    <!-- Button trigger modal -->
    <button type="button" class="course-menu-btn" data-bs-toggle="modal" data-bs-target="#maths-menu-collapse">
      Liste des chapitres
    </button>
    <div class="modal fade" id="maths-menu-collapse" tabindex="-1" aria-labelledby="exampleModalLabel"
      aria-hidden="true">
      <div class="modal-dialog modal-xl">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title text-center" id="exampleModalLabel">Liste des chapitres Maths Tle D</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
